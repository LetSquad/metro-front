{
  "name": "metro-front",
  "author": "LetSquad",
  "version": "1.0.0-MVP",
  "private": true,
  "description": "Processor of applications for assistance to disabled citizens of the Moscow metro",
  "scripts": {
    "dev": "cross-env NODE_ENV=development EXTERNAL_URL= webpack serve",
    "dev:local": "cross-env NODE_ENV=development WITH_MOCK=true webpack serve",
    "build": "npm run lint && cross-env NODE_ENV=production EXTERNAL_URL= webpack",
    "build:local": "npm run lint && cross-env NODE_ENV=production webpack",
    "bundle-analyze": "mkdir -p dist/analyze && cross-env NODE_ENV=production WITH_ANALYZER=true webpack --profile --json > dist/analyze/stats.json && webpack-bundle-analyzer dist/analyze/stats.json",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --ext .ts,.tsx src --color",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint:fix": "eslint --ext .ts,.tsx src --color --fix",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
    "husky": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint:fix",
      "git add"
    ],
    "*.scss": [
      "stylelint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.5",
    "axios": "1.7.2",
    "axios-auth-refresh": "3.3.6",
    "axios-hooks": "5.0.2",
    "axios-mock-adapter": "1.22.0",
    "date-fns": "3.6.0",
    "formik": "2.4.6",
    "lodash.get": "4.4.2",
    "lodash.isempty": "4.4.0",
    "lodash.merge": "4.6.2",
    "luxon": "3.4.4",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-avatar-editor": "13.0.2",
    "react-calendar": "5.0.0",
    "react-datepicker": "6.9.0",
    "react-dom": "18.3.1",
    "react-dropzone": "14.2.3",
    "react-hot-toast": "2.4.1",
    "react-phone-input-2": "2.15.1",
    "react-redux": "9.1.2",
    "react-responsive": "10.0.0",
    "react-router-dom": "6.23.1",
    "redux-promise-middleware": "6.2.0",
    "redux-thunk": "3.1.0",
    "semantic-ui-react": "2.1.5",
    "ts-enum-util": "4.1.0",
    "uuid": "9.0.1",
    "yup": "1.4.0",
    "zxcvbn": "4.4.2"
